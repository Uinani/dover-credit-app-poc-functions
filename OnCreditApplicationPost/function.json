{
  "bindings": [
    {
      "authLevel": "anonymous",
      "direction": "in",
      "name": "req",
      "type": "httpTrigger",
      "methods": [
        "post"
      ]
    },
    {
      "direction": "out",
      "name": "res",
      "type": "http"
    },
    {
      "direction": "out",
      "name": "$return",
      "type": "sendGrid",
      "apiKey" : "AzureWebJobsSendGridApiKey"
    }
  ]
}
